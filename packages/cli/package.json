{
  "name": "@c.a.f/cli",
  "version": "1.0.2",
  "description": "CLI tool to scaffold CAF project structure",
  "main": "dist/index.js",
  "bin": {
    "cli": "./dist/bin/caf-init.js",
    "caf-init": "./dist/bin/caf-init.js"
  },
  "directories": {
    "bin": "./dist/bin"
  },
  "scripts": {
    "build": "tsc && node dist/create-windows-wrapper.js",
    "build:windows-wrapper": "tsc src/create-windows-wrapper.ts --outDir dist --module commonjs --target ES2020 && node dist/create-windows-wrapper.js",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "caf",
    "clean-architecture",
    "scaffold",
    "cli",
    "frontend",
    "architecture"
  ],
  "author": "ali aslani <aliaslani.mm@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ialiaslani/caf.git",
    "directory": "packages/cli"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "os": [
    "win32",
    "darwin",
    "linux"
  ],
  "dependencies": {
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
